<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <script type="text/javascript" charset="utf8" src="~/DataTables-1.10.4/media/js/jquery.js"></script>
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("DataTables", required:false)
    @RenderSection("ImageGallery", required: false)

    <link rel="stylesheet" href="../Content/jquery.mThumbnailScroller.css">
</head>
<body>
    @RenderSection("ThumbnailScrollerScripts", required: false)
    @RenderBody()
    @RenderSection("Scripts", required: false)
</body>
</html>
